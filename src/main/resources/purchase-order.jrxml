		<field name="PERIOD" class="java.math.BigDecimal"/>
		<field name="TOTAL_" class="java.math.BigDecimal"/>
		<field name="PAYMENT" class="java.math.BigDecimal"/>
		<field name="PAYMENT_DATE" class="java.sql.Timestamp"/>
	</subDataset>
