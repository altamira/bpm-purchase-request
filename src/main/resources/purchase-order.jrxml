	<subDataset name="personDetailsDataset" uuid="b5c7b1c3-755a-49e9-aad1-e4f9ad6449da">
		<parameter name="USERNAME" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{USERNAME}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select name || '<br/>' || email || '<br/>' || fone as details
from
(select case when p.last_name is null then p.first_name else p.first_name || ' ' || p.last_name end as name,
       (select listagg(cm.address,'<br/>') within group (order by cm.address) as email
        from person_contact_mail pcm,
             contact_mail cm
        where pcm.person = p.id and
              pcm.contact_mail = cm.id
       ) as email,
       (select listagg( cf.type || ' +' || cf.country_code || '(' || cf.area_code || ')' || cf.prefix || '-' || cf.number_ || ' Ramal ' || cf.extension, '<br/>') within group (order by cf.type) as fone
        from person_contact_fone pcf,
             contact_fone cf
        where pcf.person = p.id and
              pcf.contact_fone = cf.id
       ) as fone
from user_preference u,
     person p
where upper(u.name) = upper($P{USERNAME})
and p.id(+) = u.person)]]>
		</queryString>
		<field name="DETAILS" class="java.lang.String"/>
	</subDataset>
