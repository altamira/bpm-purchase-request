			<defaultValueExpression><![CDATA[$P{PURCHASE_ORDER_ID}]]></defaultValueExpression>
		</parameter>
		<parameter name="DATE" class="java.sql.Timestamp"/>
		<queryString>
			<![CDATA[SELECT POY.PERCENTAGE,
       POY.PERIOD,
       TEMP.TOTAL_,
       (TEMP.TOTAL_*POY.PERCENTAGE)/100 AS PAYMENT,
       $P{DATE}+POY.PERIOD AS PAYMENT_DATE
FROM PURCHASE_ORDER_PAYMENT POY,
     (SELECT SUM(((WEIGHT*PRICE)*TAX)/100 + WEIGHT*PRICE) AS TOTAL_
      FROM PURCHASE_ORDER_ITEM POI
      WHERE POI.PURCHASE_ORDER = $P{PURCHASE_ORDER_ID}
      AND POI."DATE" = $P{DATE}) TEMP
WHERE POY.PURCHASE_ORDER = $P{PURCHASE_ORDER_ID}]]>
		</queryString>
